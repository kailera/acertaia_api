{
  "name": "my-agent-app",
  "version": "0.1.0",
  "description": "A VoltAgent application",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "volt": "volt",
    "seed": "prisma db seed",
    "db:reset": "node ./scripts/db-reset.mjs"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/cohere": "^2.0.4",
    "@ai-sdk/openai": "^2.0.11",
    "@pinecone-database/pinecone": "^6.1.2",
    "@prisma/client": "^6.15.0",
    "@qdrant/js-client-rest": "^1.15.1",
    "@supabase/supabase-js": "^2.56.0",
    "@voltagent/cli": "^0.1.9",
    "@voltagent/core": "^0.1.63",
    "@voltagent/logger": "^0.1.0",
    "@voltagent/postgres": "^0.1.12",
    "@voltagent/supabase": "^0.1.20",
    "@voltagent/vercel-ai": "^1.0.0",
    "bcryptjs": "^3.0.2",
    "cohere-ai": "^7.18.1",
    "dotenv": "^16.4.7",
    "jose": "^6.0.12",
    "openai": "^5.15.0",
    "zod": "^3.25.76",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.18.0",
    "esbuild": "^0.25.9",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
